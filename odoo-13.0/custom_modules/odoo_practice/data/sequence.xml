<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="patient_sequence" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">patient.sequence</field>
            <field name="prefix">HPP</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="doctor_sequence" model="ir.sequence">
            <field name="name">Doctor Sequence</field>
            <field name="code">doctor.sequence</field>
